/* 列表样式 */
.common-list.data-v-0a456f9e{
		padding: 20rpx;
		border-bottom: 1rpx solid #EEEEEE;
}
.common-list-l.data-v-0a456f9e{
		-webkit-flex-shrink: 0;
		    -ms-flex-negative: 0;
		        flex-shrink: 0;
}
.common-list-l image.data-v-0a456f9e{
		width: 90rpx;
		height: 90rpx;
		border-radius: 100%;
